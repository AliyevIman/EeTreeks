li>\n                  <Link className=\"text-deceration-none\" to=\"/shop\">\n                    {\" \"}\n                    <ShoppingBasketIcon />{\" \"}\n                    {cart && cart.cartitems\n                      ? cart.cartitems.reduce(\n                          (total, item) => total + item.quantity,\n                          0\n                        )\n                      : 0}\n                  </Link>\n                </li>\n                <li>\n                <Search/>\n                </li>\n                {myUser && myUser.userInfo && myUser.userInfo.token ? (\n                  <li>\n                    <Link to=\"#\">\n                      <span>{myUser.userInfo.email}</span>\n                    </Link>\n                    <button onClick={() => dispatch(logoutAction())}> Logout </button>\n                  </li>\n                ) : (\n                  <>\n                    {\" \"}\n                    <li>\n                      <Link to=\"/login\">Login</Link>\n                    </li>\n                    <li>\n                      <Link to=\"/register\">Register</Link>\n                    </li>\n                  </>\n                )}\n\n                {/* <li>\n                  <button\n                    onClick={() => dispatch(changeMode(\"#000\"))}\n                    className=\"btn btn-dark\"\n                  >\n                    Black\n                  </button>\n                </li>\n                <li>\n                  <button\n                    onClick={() => dispatch(changeMode(\"#fff\"))}\n                    className=\"btn btn-secondary\"\n                  >\n                    White\n                  </button>\n                </li> */}\n              </ul>\n            </nav>\n          </div>\n        </div>\n      </div>\n    </header>\n  );\n};\n\nexport default Header;\n","C:\\Users\\Iman\\Desktop\\udemy-web-master-sonuncu\\src\\Redux\\Reducers\\UserReducer.js",[],[],"C:\\Users\\Im